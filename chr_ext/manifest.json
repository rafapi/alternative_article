{
    "manifest_version": 3,
    "name": "URL Content Extractor",
    "version": "1.0",
    "action": {
        "default_icon": "icon.png"
    },
    "permissions": ["activeTab", "scripting", "tabs"],
    "host_permissions": ["http://localhost:8081/"],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icon.png"
    }
}

